{
  "name": "next-netlify-starter",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint ./components/** ./pages/** -c .eslintrc.json --ext js,jsx",
    "lint-fix": "eslint ./components/** ./pages/** -c .eslintrc.json --fix --ext js,jsx"
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "airtable": "^0.10.1",
    "axios": "^0.21.1",
    "netlify-plugin-cache-nextjs": "^1.5.2",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@tailwindcss/jit": "^0.1.8",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "classnames": "^2.2.6",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "globby": "^11.0.3",
    "postcss": "^8.2.8",
    "postcss-100vh-fix": "^1.0.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-hover-media-feature": "^1.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "tailwindcss-responsive-embed": "^1.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "react/display-name": 1,
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/label-has-for": [
        "off"
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    ".next/*",
    ".netlify"
  ],
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "prettierIgnore": [
    "node_modules/*",
    ".next/*",
    ".netlify/*"
  ],
  "postcss": {
    "plugins": {
      "@tailwindcss/jit": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
