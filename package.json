{
  "name": "next-netlify-starter",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build-css": "postcss src/assets/css/**/*.css --dir dist/assets/css/",
    "watch-css": "onchange 'src/assets/css/**/*.css' -- npm run build-css",
    "copy-html": "cpx 'src/**/*.html' dist",
    "copy-images": "cpx 'src/assets/images/**/*' dist/assets/images",
    "copy-js": "cpx 'src/assets/js/**/*.js' dist/assets/js/",
    "watch-html": "onchange 'src/**/*.html' -- npm run copy-html",
    "watch-js": "onchange 'src/assets/js/**/*.js' -- npm run copy-js",
    "partytown": "partytown copylib dist/~partytown",
    "generate-sitemap": "node utils/generate-sitemap.js",
    "serve": "browser-sync start --server 'dist' --files 'dist' --no-open --no-notify",
    "build": "npm-run-all clean build-css copy-html copy-images copy-js generate-sitemap partytown",
    "dev": "npm-run-all clean build --parallel watch-css watch-html watch-js serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "alpinejs": "^3.12.2",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "create-stylelint": "^0.3.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "htmlnano": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.4.2",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-include": "^1.7.4",
    "posthtml-modules": "^0.9.1",
    "prettier": "^2.8.8",
    "replace-in-files-cli": "^2.2.0",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.1",
    "stylelint": "^15.7.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "@netlify/functions": "^1.6.0",
    "dotenv": "^16.1.4",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "googleapis-common": "^6.0.4",
    "nodemailer": "^6.9.3"
  }
}